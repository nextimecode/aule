{
  "name": "aule",
  "version": "1.6.0",
  "description": "Aulë, o habilidoso artífice e mestre da criação e da forma, inspira a fundação de nosso repositório. Assim como Aulë forjou os pilares do mundo com maestria e precisão, este repositório é concebido para ser o alicerce robusto no qual sites comerciais e landing pages são criados.",
  "private": true,
  "keywords": [],
  "author": "Pedro Duarte - NeXTIME",
  "license": "MIT",
  "workspaces": [
    "apps/*",
    "libs/*"
  ],
  "scripts": {
    "vercel-build": "pnpm install && pnpm build",
    "build": "turbo run build --parallel",
    "clean:all": "turbo run clean && pnpm store prune && rm -rf .next .turbo node_modules tsconfig.tsbuildinfo pnpm-lock.yaml",
    "dev": "turbo run dev --parallel",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint --parallel",
    "audit": "pnpm audit",
    "outdated": "pnpm outdated",
    "release": "semantic-release",
    "semantic-release:ui": "semantic-release --config semantic-release-ui.config.js",
    "release:ui": "turbo run build --filter=ui && changeset publish",
    "reset:all": "pnpm clean:all && pnpm i && turbo run build --parallel",
    "start": "turbo run start",
    "typecheck": "turbo run typecheck --parallel",
    "update": "pnpm update --latest",
    "naugrim:build": "turbo run build --filter naugrim",
    "naugrim:dev": "turbo run dev --filter naugrim",
    "tumladen:build": "turbo run build --filter tumladen",
    "tumladen:dev": "turbo run dev --filter tumladen",
    "ui:build": "turbo run build --filter ui",
    "ui:dev": "turbo run dev --filter ui",
    "precommit": "turbo lint typecheck audit",
    "commit": "cz",
    "prepare": "husky install",
    "postinstall": "husky install",
    "depcheck": "depcheck"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/material": "^5.15.15",
    "@next/font": "^14.2.1",
    "@nextime-ui/icons": "^2.6.2",
    "@nextime-ui/react": "^2.6.2",
    "@nextime-ui/theme-tools": "^2.6.2",
    "@nextime-ui/tokens": "^2.6.2",
    "@preline/combobox": "^2.1.0",
    "@t3-oss/env-nextjs": "^0.9.2",
    "@tailwindcss/forms": "^0.5.7",
    "he": "^1.2.0",
    "lucide-react": "^0.368.0",
    "next": "^14.2.1",
    "next-mdx-remote": "^4.4.1",
    "next-themes": "^0.3.0",
    "preline": "^2.1.0",
    "react": "^18.2.0",
    "react-datepicker": "^6.8.0",
    "react-dom": "^18.2.0",
    "react-modal-video": "^2.0.1",
    "tailwind-merge": "^2.2.2",
    "zod": "^3.22.4",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@commitlint/cli": "^19.2.2",
    "@commitlint/config-conventional": "^19.2.2",
    "@nextime-ui/eslint-config": "^2.6.2",
    "@nextime-ui/ts-config": "^2.6.2",
    "@storybook/addon-essentials": "^8.0.8",
    "@storybook/addon-interactions": "^8.0.8",
    "@storybook/addon-links": "^8.0.8",
    "@storybook/blocks": "^8.0.8",
    "@storybook/manager-api": "^8.0.8",
    "@storybook/react": "^8.0.8",
    "@storybook/react-vite": "^8.0.8",
    "@storybook/testing-library": "^0.2.2",
    "@storybook/theming": "^8.0.8",
    "@swc/core": "^1.4.14",
    "@turbo/gen": "^1.13.2",
    "@types/he": "^1.2.3",
    "@types/jest": "^29.5.12",
    "@types/node": "20.12.7",
    "@types/prop-types": "^15.7.12",
    "@types/react": "18.2.78",
    "@types/react-datepicker": "^6.2.0",
    "@types/react-dom": "18.2.25",
    "@types/react-modal-video": "^1.2.3",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "autoprefixer": "^10.4.19",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "depcheck": "^1.4.7",
    "eslint": "^9.0.0",
    "eslint-config-next": "^14.2.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-turbo": "^1.13.2",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prop-types": "^15.8.1",
    "semantic-release": "^23.0.8",
    "semantic-release-monorepo": "^8.0.2",
    "storybook": "^8.0.8",
    "tailwindcss": "^3.4.3",
    "tsup": "^8.0.2",
    "turbo": "^1.13.2",
    "typescript": "5.4.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nextimecode/aule"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "release": {
    "branches": [
      "main"
    ]
  }
}
